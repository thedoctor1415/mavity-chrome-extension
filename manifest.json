{
  "manifest_version": 3,
  "name": "Gravity to Mavity",
  "version": "1.0",
  "description": "Replaces all instances of 'gravity' with 'mavity' and 'gravitational' with 'mavitational'",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
